{
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  },
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "rootDir": "./",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {}
  },
  "exclude": ["node_modules", "dist"]
}
